<?xml version="1.0" encoding="UTF-8"?>
<configs>
    <config name="Ignition Designer" protocol="vnc">
        <param name="hostname">designer-desktop</param>
        <param name="port">5901</param>
        <param name="password">designer</param>
        <param name="color-depth">24</param>
        <param name="cursor">local</param>
        <param name="swap-red-blue">false</param>
        <param name="enable-clipboard">true</param>
        <param name="disable-copy">false</param>
        <param name="disable-paste">false</param>
        <param name="enable-font-smoothing">true</param>
        <param name="enable-wallpaper">false</param>
        <param name="enable-theming">true</param>
        <param name="enable-desktop-composition">false</param>
        <param name="autoretry">5</param>
    </config>
</configs>
